<%- include("partials/header.ejs")  %> 
<%- include("partials/navbar.ejs")  %> 


<div class="container py-3 my-3">
  <div class="row">
    <h5>Data Gempa Terbaru (Berdasarkan BMKG) <span><a target="_blank" href="https://www.bmkg.go.id/gempabumi-dirasakan.html">selengkapnya</a></span></h5>
    <table class="table table-striped table-bordered table-hover">
      <thead class="text-center">
        <tr>
          <th scope="col">No</th>
          <th scope="col">Waktu</th>
          <th scope="col">Lokasi</th>
          <th scope="col">Kekuatan Gempa</th>
          <th scope="col">Kedalaman Gempa</th>
          <th scope="col">Jarak Pusat Gempa</th>
          <th scope="col">Dampak Gempa</th>
        </tr>
      </thead>
      <tbody class="hasil">
        <% let no=1 %> 
        <% data.forEach(d => { %>
          <tr>
            <th scope="row"><%= no++ %> </th>
            <td><%= d.Tanggal %></td>
            <td><%=  d.Wilayah %> </td>
            <td class="text-center"><%= d.Magnitude %> </td>
            <td class="text-center"><%= d.Kedalaman.split(" ")[0]  %> </td>
            <td class="text-center"><%= d.Wilayah.split(" ")[5]  %> </td>
            <td class="text-center"><%= MatchDampakGempa(d.Dirasakan) %> </td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>

<%- include("partials/footer.ejs")  %> 